    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.1.2",
    "sequelize": "^6.28.0"
  },
  "devDependencies": {
    "debug": "^4.3.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.6.0"
  }
}
